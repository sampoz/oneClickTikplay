{
  "manifest_version": 2,

  "name": "One-click Tikplay",
  "description": "This extension plays youtube-songs with TiKplay",
  "version": "1.0",

  "permissions": [
    "tabs",
    "nativeMessaging"
  ],
  "browser_action": {
    "default_icon": "tik_logo.png",
    "default_popup": "popup.html"
  }
}
